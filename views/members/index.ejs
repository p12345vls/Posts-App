<% include ../partials/headStatics %>
<main class="page ">
    <section class="clean-block clean-form dark">
        <div class="shadow">
        <h3 class="text-center p-3">AAOS Las Vegas NV 2019</h3>
        <h1 class="img-fluid text-center"><img  class="img-fluid shadow-lg" src="assets/img/vegas-meeting.png"></h1><hr>
        </div>
        <div class="col-md-12 search-table-col m-auto" style="max-width: 1000px;">
            <h3 class="text-center p-3">Active Members  </h3>
            <p class="float-left text-muted mt-n4"><small><i>sorted by first name</i></small></p>
            <div class="table-responsive table-bordered table table-hover table-bordered results border-left-0 border-right-0 border-primary">
                <table class="table table-bordered table-hover">
                    <thead class="bill-header cs">
                    <tr>
                        <th id="trs-hd" class="col-2">Name</th>
                        <th id="trs-hd" class="col-3">Role</th>
                        <th id="trs-hd" class="col-3">Address</th>
                    </tr>
                    </thead>
                    <% members.forEach(function(member){ %>
                        <tbody>
                        <tr>
                            <td><%= member.name %></td>
                            <td><%= member.role %></td>
                            <td><%= member.address %></td>
                        </tr>
                        </tbody>
                    <% }); %>
                </table>
            </div>
        </div>
    </section>
</main>


<% include ../partials/footerStatics %>

<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.3.1/js/bootstrap.bundle.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/baguettebox.js/1.10.0/baguetteBox.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/aos/2.1.1/aos.js"></script>
<script src="assets/js/script.min.js"></script>

</html>
