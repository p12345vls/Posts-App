<div class="bg-light">
<main class="page p-lg-5 mx-auto " style="max-width: 700px;">
    <section class="clean-block clean-form dark">
        <div class="container">
            <div class="block-heading">
                <h2 class="text-info text-center">Edit <%=member.name%></h2>
            </div>
            <form  class="" action="/admin1/<%= member._id %>?_method=PUT" method="POST">
                <div class="form-group"><label>First Name</label><input class="form-control"  type="text" name="member[name]" value="<%= member.name %>"></div>
                <div class="form-group"><label>Last Name</label><input class="form-control" type="text" name="member[lastname]" value="<%= member.lastname %>"></div>
                <div class="form-group"><label>Role</label><input class="form-control" name="role" type="text" name="member[role]" value="<%= member.role %>"></div>
                <div class="form-group"><label>Address</label><input class="form-control" type="text" name="member[address]" value="<%= member.address %>"></div>
                <div class="form-group">
                    <button class="btn btn-primary btn-block" type="submit">Submit</button>
                </div>
            </form>
        </div>
    </section>
